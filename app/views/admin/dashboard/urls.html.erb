<%= turbo_frame_tag "target_url_#{@target_url.id}_short_urls" do %>
  <div class="w-full px-4 py-2 sm:rounded-lg border border-gray-300">
    <% if @short_urls&.present? %>
      <%= render partial: "short_urls", object: @short_urls, locals: { url_visits: @url_visits } %>    
    <% else %>
      <div class="text-sm text-gray-500">No URLs being shortened.</div>
    <% end %>
  </div>
<% end %>